    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/git_hub_emojis_nav_graph"
    app:startDestination="@id/mainFragment">

    <fragment
        android:id="@+id/mainFragment"
        android:name="br.com.amd.githubimagecatcher.ui.MainFragment"
        android:label="MainFragment" >
        <action
            android:id="@+id/action_mainFragment_to_emojisListFragment"
            app:destination="@id/emojisListFragment" />
    </fragment>

    <fragment
        android:id="@+id/emojisListFragment"
        android:name="br.com.amd.githubimagecatcher.ui.emojislist.EmojisListFragment"
        android:label="EmojisListFragment"
        tools:layout="@layout/fragment_emojis_list" />
</navigation>